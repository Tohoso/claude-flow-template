{
  "name": "freee-receipt-flow",
  "version": "0.1.0",
  "description": "Automate accounting by processing receipt images with Gemini AI and freee API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "auth:google": "tsx src/scripts/auth-google.ts",
    "auth:freee": "tsx src/scripts/auth-freee.ts"
  },
  "keywords": [
    "freee",
    "receipt",
    "accounting",
    "automation",
    "gemini",
    "google-drive"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "better-sqlite3": "^9.4.0",
    "dotenv": "^16.4.0",
    "googleapis": "^131.0.0",
    "node-cron": "^3.0.3",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.0"
  }
}
